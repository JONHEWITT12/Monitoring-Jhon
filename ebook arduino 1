/*
 Program Animasi LED dengan PWM
*/
#define LED 3 
void setup() { 
  // mendefinikasi pin LED sebagai output 
  pinMode(LED, OUTPUT);
} 
void loop() { 
 for ( byte value = 0; value < 255; value++)
  {
  analogWrite(LED,value);
  delay(10); 
  }
for ( byte value = 255; value > 0; value--) 
 { 
 analogWrite(LED,value);
 delay(10); 
 }
}
